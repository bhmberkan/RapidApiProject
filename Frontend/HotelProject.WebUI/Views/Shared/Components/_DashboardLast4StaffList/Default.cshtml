@model List<HotelProject.WebUI.Dtos.StaffDto.ResultLast4StafDto>

<div class="row">
    @foreach (var x in Model)
    {
        <div class="col-lg-3 col-sm-6">
            <div class="card">
                <div class="card-body">
                    <div class="text-center">
                        <img src="@x.SocialMedia1" class="rounded-circle" alt="" width="75px" height="75px">
                        <h5 class="mt-3 mb-1">@x.Name</h5>
                        <p class="m-0">@x.Title</p>
                      
                    </div>
                </div>
            </div>
        </div>
    }



</div>